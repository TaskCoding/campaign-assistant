/* miniprogram/pages/merchant/campaign/analytics/index.wxss */
.container{
  padding: 20rpx;
}
.ct_groups{
  width: 100%;
}
.ct_group{
  display: flex;
  width: 100%;
}
.div_line {
  margin-bottom: 20rpx;
  background: #888;
  width: 100%;
  height: 5rpx;
}
.ct_cell{
  display: flex;
  align-content: center;
  text-align: center;
}
.weui-cell__bd{
  margin-left: 20rpx;
  font-size: 15pt;
  line-height: 100%;
  font-weight: bold;
}
.txt_table_name{
  font-size: 13pt;
  margin: 20rpx 0;
}
.table_item{
  border: 1rpx solid #353535;
  flex: 1;
  height: 60rpx;
  align-content: center;
  align-items: center;
  text-align: center;
}
.th{
  background:#888;
  font-weight: bold;
  color: #353535;
  border: 1rpx solid #353535;
  height: 60rpx;
  align-content: center;
  align-items: center;
  text-align: center;
}

.lb_created{
  flex: 0.8;
}
.txt_openid{
  flex: 1;
}
.table_item.lb_created{
  font-size: 7pt;
  align-self: center;
  text-align: center;
  line-height: 60rpx;
}
.table_item.txt_openid{
  font-size: 7pt;
  align-self: center;
  text-align: center;
  line-height: 60rpx;
}
.lb_index{
  flex: 0.3;
}